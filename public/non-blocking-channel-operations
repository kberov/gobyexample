      codeLines.push('');codeLines.push('package main\u000A');codeLines.push('import \"fmt\"\u000A');codeLines.push('func main() {\u000A    писма :\u003D make(chan string)\u000A    сигнали :\u003D make(chan bool)\u000A');codeLines.push('    select {\u000A    case писмо :\u003D \u003C-писма:\u000A        fmt.Println(\"получено писмо:\", писмо)\u000A    default:\u000A        fmt.Println(\"няма нови писма\")\u000A    }\u000A');codeLines.push('    писмо :\u003D \"hi\"\u000A    select {\u000A    case писма \u003C- писмо:\u000A        fmt.Println(\"изпратено писмо\", писмо)\u000A    default:\u000A        fmt.Println(\"не е изпратено писмо\")\u000A    }\u000A');codeLines.push('    select {\u000A    case писмо :\u003D \u003C-писма:\u000A        fmt.Println(\"получено писмо\", писмо)\u000A    case sig :\u003D \u003C-сигнали:\u000A        fmt.Println(\"получен сигнал\", sig)\u000A    default:\u000A        fmt.Println(\"бездействие\")\u000A    }\u000A}\u000A');codeLines.push('');
